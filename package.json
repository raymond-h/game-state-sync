    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "eslint": "^2.13.1",
    "eslint-config-kellyirc": "^3.1.0",
    "jsverify": "^0.7.1",
    "onchange": "^2.5.0",
    "shuffle-array": "^1.0.0"
  },
  "dependencies": {
    "lodash": "^4.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raymond-h/game-state-sync"
